<?xml version="1.0" ?>
<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
    <table name="vendor_productnotes_productnotes" resource="default" engine="innodb" comment="Vendor Product Notes">
        <column xsi:type="int" name="note_id" padding="10" unsigned="true" nullable="false" identity="true" comment="Entity Id"/>
        <constraint xsi:type="primary" referenceId="PRIMARY">
            <column name="note_id"/>
        </constraint>
        <column xsi:type="int" name="product_id" nullable="false" comment="Product ID"/>
        <column xsi:type="varchar" name="note_title" nullable="false" length="255" comment="Note Title"/>
        <column xsi:type="text" name="note_details" nullable="true" comment="Note Details"/>
        <column name="created_at" nullable="true" xsi:type="datetime" comment="created_at"/>
    </table>
</schema>
